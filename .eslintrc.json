{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "semi": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": [
      "error", {
        "semi": false
      }
    ],
    //ours
    "react/destructuring-assignment": ["off"],
    "react/prop-types": ["off"],
    "no-param-reassign": ["off"],
    "no-console": ["off"]
  }
}